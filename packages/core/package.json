{
  "name": "@spandex/core",
  "version": "0.3.1",
  "description": "SpanDEX Core: meta-aggregation for DEX swaps with quote fetching, simulation, and execution.",
  "homepage": "https://spandex.sh",
  "docs": "https://spandex.sh",
  "documentation": "https://spandex.sh",
  "repository": {
    "type": "git",
    "url": "https://github.com/withfabricxyz/spandex.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/withfabricxyz/spandex/discussions"
  },
  "license": "MIT",
  "keywords": [
    "spandex",
    "dex",
    "swap",
    "aggregator",
    "defi",
    "web3",
    "ethereum",
    "quote",
    "simulation",
    "trading-bot",
    "dapp"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun ../../scripts/build-packages.ts core"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prool": "^0.2.1"
  },
  "dependencies": {
    "viem": "^2.40"
  },
  "peerDependencies": {
    "typescript": ">=5.0.4"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
