{
  "name": "@spandex/react",
  "version": "0.4.0",
  "description": "spanDEX React: hooks for meta-aggregated DEX swaps and quote execution.",
  "homepage": "https://spandex.sh",
  "docs": "https://spandex.sh",
  "documentation": "https://spandex.sh",
  "repository": {
    "type": "git",
    "url": "https://github.com/withfabricxyz/spandex.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/withfabricxyz/spandex/discussions"
  },
  "license": "MIT",
  "keywords": [
    "spandex",
    "react",
    "dex",
    "swap",
    "aggregator",
    "defi",
    "web3",
    "ethereum",
    "quote",
    "hooks",
    "trading-bot",
    "dapp"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun ../../scripts/build-packages.ts wagmi",
    "test": "bun test --preload ./test-setup.ts"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.1",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/bun": "latest",
    "@types/react": "^19.2.8",
    "happy-dom": "^20.3.0",
    "react-dom": "^19.2.3"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.17",
    "@spandex/core": "workspace:*",
    "wagmi": "^3.3.2",
    "viem": "^2.40"
  },
  "peerDependencies": {
    "react": "^19",
    "react-dom": "^19",
    "typescript": "^5.9.3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
