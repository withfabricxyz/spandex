    "@types/react": "^19.2.8",
    "happy-dom": "^20.3.0",
    "react-dom": "^19.2.3"
  },
  "dependencies": {
    "@withfabric/spandex": "workspace:*",
    "wagmi": "^3.3.2"
  },
  "peerDependencies": {
    "react": "^19.2.3",
    "typescript": "^5.9.3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
