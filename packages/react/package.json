{
  "name": "@withfabric/spandex-react",
  "version": "0.1.0",
  "description": "React hooks for SpanDEX",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
